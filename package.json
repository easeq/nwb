{
  "name": "nwb",
  "description": "A toolkit for React, Preact & Inferno apps, React libraries and other npm modules for the web, with no configuration (until you need it)",
  "version": "0.23.0",
  "license": "MIT",
  "author": "Jonny Buchanan <jonathan.buchanan@gmail.com>",
  "bin": {
    "nwb": "./lib/bin/nwb.js"
  },
  "scripts": {
    "build": "npm run flow && npm run lint && rimraf lib && babel src --out-dir lib --quiet",
    "build:watch": "rimraf lib && babel src --watch --out-dir lib",
    "flow": "flow",
    "lint": "eslint *.js src tests",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run build && npm run test:all",
    "test:coverage": "npm run build && cross-env NODE_ENV=test NWB_TEST=true nyc mocha tests/*-test.js tests/commands/*-test.js",
    "test:all": "cross-env NODE_ENV=test NWB_TEST=true mocha --require babel-register tests/*-test.js tests/commands/*-test.js",
    "test:watch": "cross-env NODE_ENV=test NWB_TEST=true mocha --require babel-register --watch tests/*-test.js"
  },
  "files": [
    "docs",
    "lib",
    "templates",
    "express.js",
    "polyfills.js"
  ],
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "copy-template-dir": "1.4.0",
    "cross-spawn": "6.0.5",
    "debug": "4.1.1",
    "detect-port": "1.3.0",
    "figures": "2.0.0",
    "filesize": "4.1.2",
    "fs-extra": "7.0.1",
    "gzip-size": "5.0.0",
    "inquirer": "6.2.2",
    "minimist": "1.2.0",
    "ora": "3.1.0",
    "resolve": "1.10.0",
    "run-series": "1.1.8",
    "semver": "5.6.0",
    "webpack-merge": "4.2.1",
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "babel-plugin-inferno": "6.0.5",
    "babel-plugin-istanbul": "5.1.1",
    "babel-plugin-lodash": "3.3.4",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-jsx": "7.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-jsx-self": "7.2.0",
    "@babel/plugin-transform-react-jsx-source": "7.2.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "@babel/plugin-transform-runtime": "7.2.0",
    "babel-polyfill": "6.26.0",
    "@babel/preset-env": "7.3.1",
    "babel-preset-react": "6.24.1",
    "@babel/runtime": "7.3.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-noreact": "1.1.0",
    "karma": "4.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "4.0.0-rc.6",
    "phantomjs-prebuilt": "2.1.16",
    "mocha": "6.0.1",
    "expect": "24.1.0",
    "object-assign": "4.1.1",
    "promise": "8.0.2",
    "whatwg-fetch": "3.0.0",
    "webpack": "4.29.5",
    "autoprefixer": "9.4.8",
    "babel-loader": "8.0.5",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "copy-webpack-plugin": "5.0.0",
    "css-loader": "2.1.0",
    "eventsource-polyfill": "0.9.6",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "@insin/npm-install-webpack-plugin": "5.0.0",
    "opn": "5.4.0",
    "postcss-loader": "3.0.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "terser-webpack-plugin": "1.2.2",
    "webpack-dev-middleware": "3.6.0",
    "webpack-dev-server": "3.2.0",
    "webpack-hot-middleware": "2.24.3"
  },
  "devDependencies": {
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "cross-env": "5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eventsource": "1.0.7",
    "flow-bin": "0.93.0",
    "glob": "7.1.3",
    "nyc": "13.3.0",
    "rimraf": "2.6.3",
    "temp": "0.9.0",
    "tree-kill": "1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insin/nwb.git"
  },
  "keywords": [
    "development",
    "inferno",
    "preact",
    "react",
    "tooling"
  ],
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "loose": true,
          "targets": {
            "node": "6.11.5"
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "istanbul",
            {
              "include": "src"
            }
          ]
        ]
      }
    }
  },
  "nyc": {
    "instrument": false,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false
  }
}
